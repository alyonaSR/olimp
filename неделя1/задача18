#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int a, b;
    cin >> a >> b;
    if (a == 0 && b != 0 )
        cout << "NO" << endl;
    else if (b == 0 && a == 0)
        cout << "INF" << endl;
    else
        if (b % a != 0)
            cout << "NO" << endl;
        else
            cout << -(b / a) << endl;
    return 0;
}
