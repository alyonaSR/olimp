#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int x, x1, y, y1;
    cin >> x >> y >> x1 >> y1;
    if ((abs(x1-x) == 1 &&  abs(y1 - y) == 2) || (abs(x1 - x) == 2 &&  abs(y1 - y) == 1))
        cout << "YES" << endl;
    else
        cout << "NO" << endl;
    return 0;
}
