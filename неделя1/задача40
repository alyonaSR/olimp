#include <iostream>
#include <vector>
#include <set>

using namespace std;

int main() {
    int n;
    cin >> n;
    long long ans = n * (n - 1) / 2;
    vector <int> vec;
    for(int i = 0; i < n; i++){
        int a;
        cin >> a;
        vec.push_back(a);
    }
    sort(vec.begin(), vec.end());
    int s = 1;
    for(int i = 0; i < n - 1; i++){
        if(vec[i] == vec[i + 1]) {
            s++;
        }
        else{
            ans -= s * (s - 1) / 2;
            s = 1;
        }
    }
    ans -= s * (s - 1) / 2;
    cout << ans << endl;
    return 0;
}
